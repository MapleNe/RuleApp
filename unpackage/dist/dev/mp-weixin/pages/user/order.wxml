<view><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">我的订单</view><view class="action"></view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="text-tips margin-top text-center text-gray text-sm">只显示最近30条记录，请及时保存信息</view><block wx:if="{{orderList.length==0}}"><view class="no-data">暂时没有数据</view></block><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index"><view class="order-box"><view class="order-main"><view class="order-info"><text class="order-id">{{"订单ID："+item.$orig.id+item.$orig.created}}</text><block wx:if="{{item.$orig.shopInfo}}"><text class="order-type bg-blue">{{item.m0}}</text></block></view><block wx:if="{{item.$orig.shopInfo}}"><view data-event-opts="{{[['tap',[['shopInfo',['$0'],[[['orderList','',index,'shopInfo.id']]]]]]]}}" class="order-shop" bindtap="__e">{{''+item.$orig.shopInfo.title+''}}<view class="order-time">{{item.m1}}</view></view></block><block wx:if="{{!item.$orig.shopInfo}}"><view class="order-shop">[该商品已失效！]<view class="order-time">{{item.m2}}</view></view></block><block wx:if="{{item.$orig.shopInfo}}"><view class="order-btn"><text class="text-red">{{item.$orig.shopInfo.price+" 积分"}}</text><text data-event-opts="{{[['tap',[['toInfo',['$0'],[[['orderList','',index,'shopInfo']]]]]]]}}" class="text-blue" bindtap="__e">查看收费内容</text></view></block></view></view></block><block wx:if="{{isLoading==0}}"><view class="loading"><view class="loading-main"><image src="../../static/loading.gif"></image></view></view></block></view>