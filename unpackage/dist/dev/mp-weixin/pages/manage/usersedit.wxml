<view class="user"><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">用户编辑</view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><form><view class="cu-form-group margin-top"><view class="title">用户名</view><input name="input" disabled="disabled" value="{{name}}"/></view><view class="cu-form-group margin-top"><view class="title">昵称</view><input placeholder="请输入昵称" name="input" data-event-opts="{{[['input',[['__set_model',['','screenName','$event',[]]]]]]}}" value="{{screenName}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">邮箱</view><input placeholder="未设置" name="input" value="{{mail}}"/></view><view class="cu-form-group"><view class="title">网址</view><input placeholder="请输入网址" name="input" data-event-opts="{{[['input',[['__set_model',['','url','$event',[]]]]]]}}" value="{{url}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">头衔</view><input placeholder="为用户自定义头衔" name="input" data-event-opts="{{[['input',[['__set_model',['','customize','$event',[]]]]]]}}" value="{{customize}}" bindinput="__e"/></view><view class="cu-form-group margin-top"><view class="title">密码</view><input placeholder="请输入密码,不填则不修改" name="input" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">确认密码</view><input placeholder="请再次输入密码" name="input" data-event-opts="{{[['input',[['__set_model',['','repassword','$event',[]]]]]]}}" value="{{repassword}}" bindinput="__e"/></view></form><view data-event-opts="{{[['tap',[['userEdit',['$event']]]]]}}" class="post-update bg-blue" bindtap="__e"><text class="cuIcon-upload"></text></view></view>