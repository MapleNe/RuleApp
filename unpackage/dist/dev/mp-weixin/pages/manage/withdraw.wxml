<view class="user"><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">提现审核</view><view class="action"></view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="cu-card dynamic no-card" style="margin-top:20rpx;"><view class="search-type grid col-3 bg-white"><view data-event-opts="{{[['tap',[['toType',['-1']]]]]}}" class="{{['search-type-box',type=='-1'?'active':'']}}" bindtap="__e"><text>待审核</text></view><view data-event-opts="{{[['tap',[['toType',['-2']]]]]}}" class="{{['search-type-box',type=='-2'?'active':'']}}" bindtap="__e"><text>已拒绝</text></view><view data-event-opts="{{[['tap',[['toType',['0']]]]]}}" class="{{['search-type-box',type=='0'?'active':'']}}" bindtap="__e"><text>已通过</text></view></view><view class="cu-item"><view class="cu-list menu-avatar comment"><block wx:if="{{withdrawList.length==0}}"><view class="no-data">暂时没有数据</view></block><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{withdrawList.length>0}}"><view class="order-box"><view class="order-main"><view class="order-info"><text class="order-id">{{"时间："+item.m0}}</text><text class="order-type">{{"UID："+item.$orig.uid}}</text></view><view class="order-btn"><text class="text-red">{{item.$orig.num+" 积分 = ￥ "+item.$orig.num/100}}</text><text data-event-opts="{{[['tap',[['toPay',['$0'],[[['withdrawList','',index,'pay']]]]]]]}}" class="text-blue order-status" bindtap="__e">用户收款码</text></view><block wx:if="{{item.$orig.cid==-1}}"><view class="order-kill"><text data-event-opts="{{[['tap',[['toStatus',['$0',1],[[['withdrawList','',index,'id']]]]]]]}}" class="cu-btn text-green radius" bindtap="__e">已打款</text><text data-event-opts="{{[['tap',[['toStatus',['$0',0],[[['withdrawList','',index,'id']]]]]]]}}" class="cu-btn text-red radius" bindtap="__e">拒绝</text></view></block></view></view></block></block><block wx:if="{{withdrawList.length>0}}"><view data-event-opts="{{[['tap',[['loadMore',['$event']]]]]}}" class="load-more" bindtap="__e"><text>{{moreText}}</text></view></block></view></view></view><block wx:if="{{isLoading==0}}"><view class="loading"><view class="loading-main"><image src="../../static/loading.gif"></image></view></view></block></view>