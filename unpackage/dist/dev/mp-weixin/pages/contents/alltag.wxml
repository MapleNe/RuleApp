<view class="user"><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">全部标签</view><view class="action"></view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="data-box"><view class="cu-bar bg-white search"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="搜索标签" data-event-opts="{{[['input',[['__set_model',['','searchText','$event',[]]],['searchTag',['$event']]]]]}}" value="{{searchText}}" bindinput="__e"/></view></view><view class="tags"><block wx:for="{{tagList}}" wx:for-item="item" wx:for-index="index"><text data-event-opts="{{[['tap',[['toCategoryContents',['#'+item.name+'#','$0'],[[['tagList','',index,'mid']]]]]]]}}" class="tags-box" style="{{(item.style)}}" bindtap="__e">{{''+item.name+''}}</text></block></view></view></view>