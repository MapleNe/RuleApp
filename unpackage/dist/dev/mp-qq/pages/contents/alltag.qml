<view class="user"><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-back"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">全部标签<block qq:if="{{type=='edit'}}"><block>(多选)</block></block></view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="data-box"><view class="cu-bar bg-white search"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="搜索标签" data-event-opts="{{[['input',[['__set_model',['','searchText','$event',[]]],['searchTag',['$event']]]]]}}" value="{{searchText}}" bindinput="__e"/></view></view><block qq:if="{{tagList.length==0}}"><view class="no-data">暂时没有数据</view></block><view class="tags"><block qq:for="{{tagList}}" qq:for-item="item" qq:for-index="index"><text data-event-opts="{{[['tap',[['toCategoryContents',['#'+item.name+'#','$0',index],[[['tagList','',index,'mid']]]]]]]}}" class="{{['tags-box',item.active==1?'active':'']}}" bindtap="__e">{{''+item.name+''}}</text></block></view></view><block qq:if="{{type=='edit'}}"><block><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="post-update bg-blue" bindtap="__e"><text class="cuIcon-upload"></text></view></block></block><block qq:if="{{isLoading==0}}"><view class="loading"><view class="loading-main"><image src="../../static/loading.gif"></image></view></view></block></view>