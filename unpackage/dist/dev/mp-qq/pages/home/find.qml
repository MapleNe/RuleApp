<view><view class="header" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="cu-bar bg-white" style="{{'height:'+(CustomBar+'px')+';'+('padding-top:'+(StatusBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['toSearch',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-search"></text></view><view class="content text-bold" style="{{'top:'+(StatusBar+'px')+';'}}">发现</view></view></view><view style="{{'padding:'+(NavBar+'px 10px 0px 10px')+';'}}"></view><view class="data-box"><view class="cu-bar bg-white"><view class="action data-box-title"><text class="cuIcon-titles text-rule"></text>排行榜</view><view data-event-opts="{{[['tap',[['toTopContents',['排行榜','commentsNum']]]]]}}" class="action more" bindtap="__e"><text>更多热门</text><text class="cuIcon-right"></text></view></view><view class="top"><block qq:for="{{topList}}" qq:for-item="item" qq:for-index="index" qq:key="index"><view data-event-opts="{{[['tap',[['toInfo',['$0'],[[['topList','',index]]]]]]]}}" class="top-box" bindtap="__e"><text>{{index+1}}</text>{{item.title+''}}</view></block></view></view><view class="data-box"><view class="cu-bar bg-white"><view class="action data-box-title"><text class="cuIcon-titles text-rule"></text>内容分类</view><view data-event-opts="{{[['tap',[['toAllcategory',['$event']]]]]}}" class="action more" bindtap="__e"><text>全部分类</text><text class="cuIcon-right"></text></view></view><view class="category grid col-3"><block qq:for="{{metaList}}" qq:for-item="item" qq:for-index="index"><block qq:if="{{item.parent==0}}"><view data-event-opts="{{[['tap',[['toCategoryContents',['$0','$1'],[[['metaList','',index,'name']],[['metaList','',index,'mid']]]]]]]}}" class="category-box" bindtap="__e"><view class="category-main">{{''+item.name+''}}</view></view></block></block></view></view><view class="data-box"><view class="cu-bar bg-white"><view class="action data-box-title"><text class="cuIcon-titles text-rule"></text>标签云</view><view data-event-opts="{{[['tap',[['toAlltag',['$event']]]]]}}" class="action more" bindtap="__e"><text>更多标签</text><text class="cuIcon-right"></text></view></view><view class="tags"><block qq:for="{{tagList}}" qq:for-item="item" qq:for-index="index"><text data-event-opts="{{[['tap',[['toCategoryContents',['#'+item.name+'#','$0'],[[['tagList','',index,'mid']]]]]]]}}" class="tags-box" bindtap="__e">{{''+item.name+''}}</text></block></view></view><block qq:if="{{ads!=''}}"><view class="ads-box"><image src="{{ads[0]}}" mode="widthFix" data-event-opts="{{[['tap',[['toAds',['$0'],['ads.__$n1']]]]]}}" bindtap="__e"></image></view></block><block qq:if="{{isLoading==0}}"><view class="loading"><view class="loading-main"><image src="../../static/loading.gif"></image></view></view></block></view>